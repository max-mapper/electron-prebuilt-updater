{
  "name": "electron-prebuilt-updater",
  "repository": "https://github.com/johnmuhl/electron-prebuilt-updater",
  "env": {
    "API_KEY": "echo -n 'npm_username:npm_password' | base64",
    "EMAIL": "npm email address",
    "OWNER": "github username",
    "REPO": "github repository name",
    "SECRET": "github webhook secret",
    "TOKEN": "github personal access token with public_repo scope"
  }
}
